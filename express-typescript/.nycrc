{
  "extends": "@istanbuljs/nyc-config-typescript",
  "reporter": [
    "lcov",
    "text",
    "html"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "temp-dir": "./tmp/.nyc_output",
  "report-dir": "./tmp/.test-coverage",
  "sourceMap": true,
  "all": true,
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 90,
  "check-coverage": true
}
